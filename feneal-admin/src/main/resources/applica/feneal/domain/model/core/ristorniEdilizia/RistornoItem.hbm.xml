<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="applica.feneal.domain.model.core.ristorniEdilizia.RistornoItem" table="fenealweb_ristornoitem" >
        <id name="id" type="long">
            <generator class="native" />
        </id>
        <property name="importoTot" />
        <many-to-one name="referente" class="applica.feneal.domain.model.core.ristorniEdilizia.Referenti" column="contactId" lazy="false" />
        <many-to-one name="idRistorno" class="applica.feneal.domain.model.core.ristorniEdilizia.Ristorno" column="ristornoId" lazy="false" />


    </class>
</hibernate-mapping>
