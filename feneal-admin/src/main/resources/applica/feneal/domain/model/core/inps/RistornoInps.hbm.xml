<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="applica.feneal.domain.model.core.inps.RistornoInps" table="fenealweb_ristorni" >
        <id name="id" type="long">
            <generator class="native" />
        </id>
        <property name="companyId" type="long" />

        <property name="data" />
        <property name="titolo" />
        <property name="percentualeRistorno" />
        <property name="percentualeRistornoEdili" />
        <property name="costoInpsRiga" />
        <property name="percentualeEffettiva"></property>
