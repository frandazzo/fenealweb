<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="applica.feneal.domain.model.setting.option.ApplicationOptions" table="applicationOptionss" >
		<id name="id" type="long">
			<generator class="native" />
		</id>
		<property name="createDelegaAsAccettata" />
		<property name="updateFirmasDuringImport" />
		<property name="updateWorkersDuringImport" />
		<property name="creaDelegaIfNotExistDuringImport" />
		<property name="associaDelegaDuringImport" />
		<property name="companyId" type="long" />
	</class>
</hibernate-mapping>
